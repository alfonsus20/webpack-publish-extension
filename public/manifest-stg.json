{
  "manifest_version": 3,
  "name": "My To Do App",
  "version": "1.2.9",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icons/icon-128x128.png"
    }
  },
  "background": {
    "service_worker": "background.bundle.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.bundle.js"
      ]
    }
  ],
  "permissions": [
    "notifications"
  ]
}